	.equ N,3
	.equ M,3
	.equ @,$000
	.equ STACKTOP, $E00
	.equ AT_address, $E07

	; use CBA method for arrays since only have to pass address of the array to be transposed

	lds#	STACKTOP	;top of stap
	lds	matrix
	lds#	N
	lds#	M
	lds	AT_address
	tsf
	jsr	transpose
	ads#	2	;destroy stack 
	hlt

matrix:	.blkw	N*M
AT_adrdress:	.blkw M*N

